<div class="post-outer" data-post-title="<%= page.title %>" id="post">
  <section class="post-toc">
    <%- partial('commons/sidebar/sidebar-left', {post: page}) %>
  </section>
  <section class="post-main">
    <%- partial('commons/article', {post: page, index: false}) %>
    <% if (theme.gitalk.enabled){ %>
      <%- partial('includes/gitalk') %>
    <% } %>
  </section>
  <section class="post-sidebar">
    <%- partial('commons/sidebar/sidebar-right', null, {cache: !config.relative_link}) %>
  </section>
</div>
